{
  "name": "angular-template",
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "electron": "concurrently \"ng build\" \"tsc -p tsconfig.electron.json\" && electron ./dist/electron/main.js",
    "tsc": "tsc -p tsconfig.electron.json",
    "test": "jest --no-cache",
    "build-app": "ng build && electron-builder",
    "dist": "electron-builder --dir",
    "pack": "electron-builder --dir --win --x64",
    "release": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular/compiler": "^15.0.0",
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@types/electron-builder": "^2.8.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.3",
    "typescript": "~4.8.2",
    "@types/debug": "^4.1.7"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup"
  }
}
